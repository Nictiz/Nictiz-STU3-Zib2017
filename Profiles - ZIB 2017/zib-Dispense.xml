      <alias value="Verstrekker" />
      <mapping>
        <identity value="hcim-dispense-v1.0-2017EN" />
        <map value="NL-CM:9.9.20858" />
        <comment value="Supplier" />
      </mapping>
      <mapping>
        <identity value="hcim-basicelements-v1.0-2017EN" />
        <map value="NL-CM:0.0.7" />
        <comment value="Author" />
      </mapping>
    </element>
