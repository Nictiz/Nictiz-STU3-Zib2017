      <alias value="VerstrekkingsDatumTijd" />
      <example>
        <label value="Example of DispenseDateTime" />
        <valueDateTime value="2017-05-17" />
      </example>
      <mapping>
        <identity value="hcim-dispense-v1.0-2017EN" />
        <map value="NL-CM:9.9.20272" />
